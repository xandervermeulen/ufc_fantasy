{
	"name": "ufc-fantasy-frontend",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"sync-types": "cd .. && make sync-types",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write --log-level error .",
		"all": "npm run sync-types && npm run format && npm run lint && npm run check"
	},
	"dependencies": {
		"@jill64/sentry-sveltekit-cloudflare": "^2.0.5",
		"@tailwindcss/forms": "^0.5.10",
		"js-cookie": "^3.0.5",
		"openapi-fetch": "^0.13.4"
	},
	"devDependencies": {
		"@sentry/vite-plugin": "^3.0.0",
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tanstack/eslint-plugin-query": "^5.64.2",
		"@tanstack/svelte-query": "^5.64.2",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^22.10.7",
		"@typescript-eslint/eslint-plugin": "^8.20.0",
		"@typescript-eslint/parser": "^8.20.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"js-yaml": "^4.1.0",
		"node-fetch": "^3.3.2",
		"openapi-typescript": "7.5.2",
		"postcss": "^8.5.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"svelte": "^5.19.0",
		"svelte-check": "^4.1.4",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"vite": "^5.0.0"
	}
}
